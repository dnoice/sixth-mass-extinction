# =============================================================================
# FIGURE SETTINGS
# =============================================================================
# Base: Clearcut Horizon from Terrestrial extraction
figure.figsize:     12, 6
figure.dpi:         100
figure.facecolor:   C4B493
figure.edgecolor:   A59674
figure.frameon:     True
figure.autolayout:  False


# =============================================================================
# FONT SETTINGS
# =============================================================================
# Serif for documentary weight; Georgia primary, system fallbacks
font.family:        serif
font.serif:         Georgia, Palatino Linotype, Book Antiqua, Palatino, Times New Roman, serif
font.size:          11
font.weight:        normal


# =============================================================================
# AXES SETTINGS
# =============================================================================
# Face: Clearcut Horizon | Edge: Stump Ring midtone
axes.facecolor:     C4B493
axes.edgecolor:     6D6046
axes.linewidth:     1.2
axes.grid:          True
axes.grid.axis:     both
axes.grid.which:    major
axes.axisbelow:     True

# Title: Poacher's Trail dark
axes.titlesize:     14
axes.titleweight:   bold
axes.titlecolor:    3B3527
axes.titlepad:      12.0

# Labels
